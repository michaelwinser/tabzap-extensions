{
    "manifest_version": 3,
    "name": "TabZap Select",
    "version": "1.0",
    "description": "Quickly select multiple tabs and perform batch operations",
    "action": {
        "default_icon": "icon48.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs", "bookmarks"
    ]
}
